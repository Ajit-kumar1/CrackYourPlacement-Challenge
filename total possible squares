import java.util.Scanner;
class Chess
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int sum =0;
        int n = sc.nextInt();
        if(n<0)
        System.out.println("NULL");
        else
        {
        while(n>0)
        {
            sum=sum+(n*n);
            n--;
        }
        System.out.println(sum);
    }
    }
}
